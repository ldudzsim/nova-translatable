{
	"name": "jesperfiltenborg/nova-variable-field",
	"description": "A Laravel Nova field has the opportunity to contain variable values.",
	"keywords": [
		"laravel",
		"nova",
		"translatable"
	],
	"license": "MIT",
	"require": {
		"php": "^8.1",
		"spatie/laravel-translatable": "^4.0|^5.0"
	},
	"autoload": {
		"psr-4": {
			"JesperFiltenborg\\Translatable\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"JesperFiltenborg\\Translatable\\FieldServiceProvider"
			]
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
